
SELECT
    t.Name,
    t.Domain,
    t.REPORT_BATCH_FILE_NAME,
    (
        SELECT
            LISTAGG(v.Data, '') WITHIN GROUP (ORDER BY v.Data) AS Data
        FROM
        (
            SELECT v1.Data
            FROM V_TRB_REP_ETD_TRD_DTCC_ESMA_RFT v1
            WHERE t.REPORT_BATCH_FILE_NAME = v1.REPORT_BATCH_FILE_NAME
            AND v1.FILE_LINE_NO = -1
            AND (t.Domain = 'TOPAZ' OR t.Domain = 'GMI_ETD')

            UNION ALL

            SELECT v2.Data
            FROM V_TRB_REP_ETD_POS_DTCC_ESMA_RFT v2
            WHERE t.REPORT_BATCH_FILE_NAME = v2.REPORT_BATCH_FILE_NAME
            AND v2.FILE_LINE_NO = -1
            AND (t.Domain = 'TOPAZ_POS' OR t.Domain = 'GMI_ETD_POS')

            UNION ALL

            SELECT v3.Data
            FROM V_TRB_REP_OTC_TRD_DTCC_ESMA_RFT v3
            WHERE t.REPORT_BATCH_FILE_NAME = v3.REPORT_BATCH_FILE_NAME
            AND v3.FILE_LINE_NO = -1
            AND t.Domain = 'GMI_OTC'

            UNION ALL

            SELECT v4.Data
            FROM V_TRB_REP_ETD_TRD_DTCC_ESMA_RFT v4
            WHERE t.REPORT_BATCH_FILE_NAME = v4.REPORT_BATCH_FILE_NAME
            AND v4.FILE_LINE_NO = 9999999
            AND (t.Domain = 'TOPAZ' OR t.Domain = 'GMI_ETD')

            UNION ALL

            SELECT v5.Data
            FROM V_TRB_REP_ETD_POS_DTCC_ESMA_RFT v5
            WHERE t.REPORT_BATCH_FILE_NAME = v5.REPORT_BATCH_FILE_NAME
            AND v5.FILE_LINE_NO = 9299999
            AND (t.Domain = 'TOPAZ_POS' OR t.Domain = 'GMI_ETD_POS')

            UNION ALL

            SELECT v6.Data
            FROM V_TRB_REP_OTC_TRD_DTCC_ESMA_RFT v6
            WHERE t.REPORT_BATCH_FILE_NAME = v6.REPORT_BATCH_FILE_NAME
            AND v6.FILE_LINE_NO = 9999998
            AND t.Domain = 'GMI_OTC'
        ) v
    ) AS Data
FROM
    TRB_REPORT_DICC_ESMA_RFT t;
